{
    "manifest_version": 2,
    "name": "FlowBuddy",
    "description": "Create a new moodboard from any image",
    "version": "1.0",
    "permissions": ["contextMenus", "activeTab", "tabs", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
}